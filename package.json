{
  "name": "mable-dashboard",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://dashboard.dev.mable.ai",
  "scripts": {
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:integration": "env-cmd -f .env.integration next build",
    "build:development": "env-cmd -f .env.development next build",
    "build:production": "env-cmd -f .env.production next build",
    "test": "next test",
    "eject": "next eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write . --config ./.prettierrc",
    "format:check": "prettier --check . --config ./.prettierrc",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.4",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@reduxjs/toolkit": "^1.8.3",
    "axios": "^0.27.2",
    "chart.js": "^3.8.2",
    "chartjs-plugin-datalabels": "^2.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "export-to-csv": "^0.2.1",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "moment": "^2.29.4",
    "next": "13.0.2",
    "next-redux-wrapper": "^8.0.0",
    "react": "18.2.0",
    "react-chartjs-2": "^4.3.1",
    "react-date-range": "^1.4.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux-thunk": "^2.4.1",
    "styled-components": "^5.3.6",
    "typescript": "4.8.4",
    "web-vitals": "^2.1.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-date-range": "^1.4.4",
    "@types/react-dates": "^21.8.3",
    "@types/react-dom": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx}": [
      "npm lint:fix",
      "npm format"
    ]
  }
}
